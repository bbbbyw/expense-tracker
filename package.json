{
  "name": "expense-tracker",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "nodemon --watch server --exec ts-node --project server/tsconfig.json server/index.ts",
    "dev:client": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "start:server": "ts-node server/index.ts",
    "start:client": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "next": "^14.2.5",
    "@prisma/client": "^5.19.1",
    "prisma": "^5.19.1",
    "recharts": "^2.12.7",
    "date-fns": "^3.6.0",
    "zod": "^3.23.8",
    "react-hook-form": "^7.52.1",
    "@hookform/resolvers": "^3.9.0",
    "lucide-react": "^0.427.0",
    "clsx": "^2.1.1",
    "tailwind-merge": "^2.5.2",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "@vercel/node": "^3.0.7"
  },
  "devDependencies": {
    "typescript": "^5.5.4",
    "@types/node": "^20.14.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.40",
    "tailwindcss": "^3.4.7",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.5",
    "ts-node": "^10.9.2",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "ts-node-dev": "^2.0.0",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  }
}

